#include <stdio.h>
#include <string.h>
#define MAXLINE 1000
int getline(char *line, int max);
//find: imprime líneas que coinciden con el patron del 1er argumento
main(int argc, char *argv[])
{
char line[MAXLINE];
int found=0;
if (argc!=2)
printf("Uso: find patrón\n");
else
while (getline(line, MAXLINE)>0)
if (strstr(line, argv[1]!=NULL)){
printf("%s", line);
found++;
}
return found;
}
