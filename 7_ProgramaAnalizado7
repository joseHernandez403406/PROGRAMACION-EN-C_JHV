#include <stdio.h>
main()
{
int x=3, y;
float z;
y=x+3;
printf ("%d\n", y);
y=3*x*x + 2*x + 1;
printf ("%d\n", y);
z=x*0.3;
printf ("%f\n", z);
z=y/x;
printf ("%f\n", z);
z=(float) y/x;
printf ("%f\n", z);
}
